{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env"
    ]
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/transform-runtime",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    "loadable-components/babel",
    ["import", { "libraryName": "antd", "style": true }]
  ]
}
